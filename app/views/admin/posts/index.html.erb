<section id="page-title">
  <div class="container clearfix">
    <h1><%= t ".datatable" %></h1>
  </div>
</section>

<section id="content">
  <div class="content-wrap">
    <div class="container clearfix">
      <div class="table-responsive">
      <table id="datatable1" class="table table-striped table-bordered"
        cellspacing="0" width="100%">
        <thead>
          <tr>
            <th><%= t ".id" %></th>
            <th><%= t ".title" %></th>
            <th><%= t ".user_id" %></th>
            <th><%= t ".picture" %></th>
            <th><%= t ".video" %></th>
            <th><%= t ".created_at" %></th>
            <th><%= t ".updated_at" %></th>
            <th><%= t ".delete" %></th>
          </tr>
        </thead>
        <tbody>
          <%= paginate @posts %>
          <%= render @posts %>
        </tbody>
        </table>
      </div>
    </div>
  </div>
</section>
