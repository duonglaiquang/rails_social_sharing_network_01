<% provide :title, t("sessions.new.login") %>
<% provide :name, t("sessions.new.login") %>
<section id="content">
  <div class="content-wrap">
    <div class="container clearfix">
      <div class="accordion accordion-lg divcenter nobottommargin clearfix">
        <div class="acctitle"><i class="acc-closed icon-lock3"></i>
          <i class="acc-open icon-unlock"></i>
          <%= t "sessions.new.login_to_your_acc" %>
          <%= link_to t("sessions.new.sign_in_google"), "/auth/google_oauth2" %>
        </div>
        <div class="clearfix">
          <%= render "login_form" %>
        </div>
      </div>
    </div>
  </div>
</section>
